<?xml version = '1.0'?>
<PropertyList>
  <path>clock.ac</path>
  <animation>
    <type>pick</type>
    <object-name>flybackknob</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>F4.clockResetexport()</script>
      </binding>
    </action>
  </animation>
    
  <animation>
    <type>rotate</type>
    <object-name>minute-sweep</object-name>
    <property>instrumentation/clock/stopwatch-seconds</property>
    <factor>0.1</factor>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>second-sweep</object-name>
    <property>instrumentation/clock/stopwatch-seconds</property>
    <factor>6</factor>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>minute</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>0.1</factor>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>hour</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>0.008333</factor>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <object-name>hour</object-name>
    <type>material</type>
    <emission>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
      <red>0.45</red>
      <green>0.34</green>
      <blue>0.18</blue>
    </emission>
  </animation>
  <animation>
    <object-name>minute</object-name>
    <type>material</type>
    <emission>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
      <red>0.45</red>
      <green>0.34</green>
      <blue>0.18</blue>
    </emission>
  </animation>
  <animation>
    <object-name>second-sweep</object-name>
    <type>material</type>
    <emission>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
      <red>0.45</red>
      <green>0.34</green>
      <blue>0.18</blue>
    </emission>
  </animation>
  <animation>
    <object-name>minute-sweep</object-name>
    <type>material</type>
    <emission>
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
      <red>0.45</red>
      <green>0.34</green>
      <blue>0.18</blue>
    </emission>
  </animation>
</PropertyList>
