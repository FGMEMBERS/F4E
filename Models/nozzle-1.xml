<?xml version="1.0"?>
<PropertyList>

 <path>nozzle.ac</path>
    <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
     <property>engines/engine[1]/nozzle-pos-norm</property>
     <interpolation>
      <entry> <ind>0.00</ind> <dep> 40</dep> </entry>
      <entry> <ind>0.25</ind> <dep> 15</dep> </entry>
      <entry> <ind>0.50</ind> <dep>  0</dep> </entry>
      <entry> <ind>0.75</ind> <dep>-15</dep> </entry>
      <entry> <ind>1.00</ind> <dep>-40</dep> </entry>
     </interpolation>

       <center>
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>-0.0125</z-m>
       </center>
    <axis>
      <x>-1</x>
      <y> 0</y>
      <z> 0</z>
    </axis>
  </animation>
  <animation> 
   <object-name>scale</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

 <animation> 
   <object-name>needle</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>


</PropertyList>
