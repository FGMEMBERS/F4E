<?xml version="1.0"?>

<PropertyList>

 <name>ASI</name>
 <path>A-10-AoA-gauge.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>/orientation/alpha-deg</property>
  <interpolation>
   <!--        AoA deg           gauge deg       AoA units -->
   <entry><ind>-10</ind><dep> 270</dep></entry>  <!--  0.0 -->
   <entry><ind>  0</ind><dep> 180</dep></entry>  <!-- 10.0 -->
   <entry><ind> 15</ind><dep>  90</dep></entry>  <!-- 20.0 -->
   <entry><ind> 40</ind><dep>   0</dep></entry>  <!-- 30.0 -->

  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


<!-- lighting -->

  <animation> 
   <object-name>mount</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation>
   <object-name>face</object-name>
   <type>material</type>
     <emission> 
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
        <red>0.50</red>
        <green>0.25</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation>
   <object-name>needle</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
        <red>0.75</red>
        <green>0.55</green>
        <blue>0.35</blue>
      </emission>
  </animation>


 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>

</PropertyList>


