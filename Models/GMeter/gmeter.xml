<?xml version="1.0"?>

<!-- pilot-g -->

<PropertyList>

 <path>gmeter.ac</path>
<animation>
  <type>pick</type>
  <object-name>reset</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-assign</command>
      <property>controls/gmeter/reset</property>
      <value>0</value>
    </binding>
  </action>
</animation>

 <animation>
  <type>rotate</type>
  <object-name>g-Needle1</object-name>
  <property>accelerations/pilot-g</property>
  <interpolation>
   <entry><ind>-4</ind><dep>-200</dep></entry>
   <entry><ind>-2</ind><dep>-156</dep></entry>
   <entry><ind>-1</ind><dep>-134</dep></entry>
   <entry><ind>0</ind><dep>-112</dep></entry>
   <entry><ind>1</ind><dep>-90</dep></entry>
   <entry><ind>2</ind><dep>-68</dep></entry>
   <entry><ind>3</ind><dep>-46</dep></entry>
   <entry><ind>4</ind><dep>-24</dep></entry>
   <entry><ind>5</ind><dep>-2</dep></entry>
   <entry><ind>6</ind><dep>20</dep></entry>
   <entry><ind>8</ind><dep>64</dep></entry>
   <entry><ind>9</ind><dep>86</dep></entry>
   <entry><ind>10</ind><dep>107.5</dep></entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center> 
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>g-Needle2</object-name>
  <property>accelerations/pilot-gmin</property>
  <interpolation>
   <entry><ind>-4</ind><dep>-200</dep></entry>
   <entry><ind>-2</ind><dep>-156</dep></entry>
   <entry><ind>-1</ind><dep>-134</dep></entry>
   <entry><ind>0</ind><dep>-112</dep></entry>
   <entry><ind>1</ind><dep>-90</dep></entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center> 
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>g-Needle3</object-name>
  <property>accelerations/pilot-gmax</property>
  <interpolation>
   <entry><ind>1</ind><dep>-90</dep></entry>
   <entry><ind>2</ind><dep>-68</dep></entry>
   <entry><ind>3</ind><dep>-46</dep></entry>
   <entry><ind>4</ind><dep>-24</dep></entry>
   <entry><ind>5</ind><dep>-2</dep></entry>
   <entry><ind>6</ind><dep>20</dep></entry>
   <entry><ind>8</ind><dep>64</dep></entry>
   <entry><ind>9</ind><dep>86</dep></entry>
   <entry><ind>10</ind><dep>107.5</dep></entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center> 
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 <animation>
  <type>material</type>
  <object-name>Face</object-name>
   <emission>
     <red>0.6</red>
     <green>0.3</green>
     <blue>0.3</blue>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
   </emission> 
 </animation> 

 <animation>
  <type>material</type>
  <object-name>g-Needle1</object-name>
   <emission>
     <red>0.6</red>
     <green>0.3</green>
     <blue>0.3</blue>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
   </emission> 
 </animation> 

