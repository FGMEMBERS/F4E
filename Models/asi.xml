<?xml version="1.0"?>
<PropertyList>

 <path>asi.ac</path>
    <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property>/velocities/airspeed-kt</property>

    <min>50.0</min>
    <max>590.0</max>
    <scale>1.0</scale>
    <interpolation>
       <entry><ind>   0</ind><dep>    0</dep></entry>
       <entry><ind>  80</ind><dep>   28</dep></entry>
       <entry><ind> 100</ind><dep>   45</dep></entry>
       <entry><ind> 120</ind><dep>   64</dep></entry>
       <entry><ind> 150</ind><dep>   91</dep></entry>
       <entry><ind> 200</ind><dep>  132</dep></entry>
       <entry><ind> 250</ind><dep>  159</dep></entry>
       <entry><ind> 300</ind><dep>  184</dep></entry>
       <entry><ind> 400</ind><dep>  222</dep></entry>
       <entry><ind> 500</ind><dep>  254</dep></entry>
       <entry><ind> 600</ind><dep>  280</dep></entry>
       <entry><ind> 700</ind><dep>  305</dep></entry>
       <entry><ind> 800</ind><dep>  328</dep></entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y> 0</y>
      <z> 0</z>
    </axis>
  </animation>

    <animation>
    <type>texrotate</type>
    <object-name>mach</object-name>
    <property>/instrumentation/altimeter/indicated-altitude-ft</property>

    <min>0.0</min>
    <max>35000.0</max>
    <scale>1.0</scale>
    <interpolation>
       <!--       Altitude      Gauge Angle        -->
       <entry><ind>    0</ind><dep>   -6</dep></entry>
       <entry><ind> 5000</ind><dep>  -19</dep></entry>
       <entry><ind>10000</ind><dep>  -32</dep></entry>
       <entry><ind>15000</ind><dep>  -43</dep></entry>
       <entry><ind>20000</ind><dep>  -58</dep></entry>
       <entry><ind>25000</ind><dep>  -70</dep></entry>
       <entry><ind>30000</ind><dep>  -86</dep></entry>
       <entry><ind>35000</ind><dep>  -96</dep></entry>
       <entry><ind>40000</ind><dep> -116</dep></entry>
       <entry><ind>45000</ind><dep> -130</dep></entry>
       <entry><ind>50000</ind><dep> -150</dep></entry>
    </interpolation>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
  <center>
   <x>0.5</x>
   <y>0.5</y>
  </center>
  </animation>

<!-- lighting -->

  <animation> 
   <object-name>bezel</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation>
   <object-name>knots</object-name>
   <type>material</type>
     <emission> 
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
        <red>0.50</red>
        <green>0.25</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation>
   <object-name>mach</object-name>
   <type>material</type>
     <emission> 
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
        <red>0.50</red>
        <green>0.25</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation>
   <object-name>Needle</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
        <red>0.75</red>
        <green>0.55</green>
        <blue>0.35</blue>
      </emission>
  </animation>
</PropertyList>
