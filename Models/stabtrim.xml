<?xml version="1.0"?>
<PropertyList>

 <path>stabtrim.ac</path>
    <animation>
    <type>rotate</type>
    <object-name>bigneedle</object-name>
     <property>controls/flight/elevator-trim</property>
  <interpolation>
   <!--        trim        gauge deg-->
   <entry><ind> 1</ind><dep>-45</dep></entry>
   <entry><ind> 0</ind><dep>  0</dep></entry>
   <entry><ind>-1</ind><dep> 45</dep></entry>
  </interpolation>
       <center>
        <x-m>0</x-m>
        <y-m> 0.0161</y-m>
        <z-m> 0</z-m>
       </center>
    <axis>
      <x>-1</x>
      <y> 0</y>
      <z> 0</z>
    </axis>
  </animation>
  <animation> 
   <object-name>scale</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation> 
   <object-name>bigneedle</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/instruments-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

</PropertyList>
